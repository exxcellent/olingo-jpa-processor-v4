<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChange354wideDual"
			editable="true">
			<f:title>
				<core:Title text="Business Partner" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="4"
					labelSpanL="3"
					labelSpanM="4"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="0"
					emptySpanL="4"
					emptySpanM="0"
					emptySpanS="0"
					columnsXL="2"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer title="General Data">
					<f:formElements>
						<f:FormElement label="ID">
							<f:fields>
								<Input value="{demoModel>ID}" enabled="false"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="ETag">
							<f:fields>
								<Input value="{demoModel>ETag}" enabled="false"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Type">
							<f:fields>
								<Input value="{demoModel>Type}" enabled="false"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Country">
							<f:fields>
								<Input value="{demoModel>Country}" enabled="false"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Region">
							<f:fields>
								<Input value="{local>/changedRegion}"  placeholder="{demoModel>Address/Region}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Postal Code">
							<f:fields>
								<Input value="{local>/changedPostalCode}"  placeholder="{demoModel>Address/PostalCode}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="City name">
							<f:fields>
								<Input value="{local>/changedCityName}"  placeholder="{demoModel>Address/CityName}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Street name">
							<f:fields>
								<Input value="{local>/changedStreetName}"  placeholder="{demoModel>Address/StreetName}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="House number">
							<f:fields>
								<Input value="{local>/changedHouseNumber}"  placeholder="{demoModel>Address/HouseNumber}"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="CreationDateTime">
							<f:fields>
								<Input value="{demoModel>CreationDateTime}"  enabled="false"/>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>